# This file contains test records for RackProt and RackTemps.


# Rack protection system status
# Rack protection status bit for each crate
# "Test rack" for development and testing
# 

record(bi, "$(detector)_RackProt_TEST_1_0/state")
{
        field(DTYP,"RackProt")
	field(INP,"1")
	field(SCAN,".5 second")
        field(DESC,"Rack Protection Status")
	field(ZNAM,"NOT_GOOD")
	field(ONAM,"Good")
	field(ZSV,"MAJOR")
	field(OSV,"NO_ALARM")
	field(COSV,"NO_ALARM")
}

